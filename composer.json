{
	"name": "ijvo/google",
	"type": "library",
	"description": "Google API wrapper and authentication for Nette Framework",
	"keywords": ["nette", "google", "authentication", "auth", "login"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Mikuláš Dítě",
			"email": "mikulas@dite.pro"
		},
		{
			"name": "Filip Procházka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		},
		{
			"name": "Ing. Jaroslav Vaculik -IJVo",
			"email": "ijvo.ijvo@gmail.com",
			"homepage": "https://ijvo.cz"
		},
		{
			"name": "Google",
			"homepage": "https://github.com/google/google-api-php-client/graphs/contributors"
		}
	],
	"require": {
		"php": ">=8.1",
		"ext-curl": "*",
		"ext-json": "*",

		"nette/application": "~3.2.0",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.1",
		"nette/http": "^3.3",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.1",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.9",

		"google/apiclient": "^2.0"
	},
	"require-dev": {
		"nette/tester": "^2.5"
	},
	"autoload": {
		"psr-0": {
			"Kdyby\\Google\\": "src/"
		},
		"classmap": [
			"src/Kdyby/Google/exceptions.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/KdybyTests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1-dev"
		}
	}
}
